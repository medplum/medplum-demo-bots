{
  "type": "module",
  "name": "medplum-demo-bots",
  "version": "1.0.0",
  "description": "Medplum Demo Bots",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
